{
  "name": "sound-buttons",
  "version": "1.0.0",
  "description": "音效按钮",
  "author": "Your Name",
  "main": "main.js",
  "private": true,
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --mac --publish never"
  },
  "devDependencies": {
    "electron": "^20.0.0",
    "electron-builder": "^24.0.0"
  },
  "build": {
    "appId": "com.soundbuttons.app",
    "productName": "音效按钮",
    "directories": {
      "output": "dist"
    },
    "publish": null,
    "mac": {
      "target": "dmg",
      "arch": ["x64", "arm64"],
      "category": "public.app.category.utilities",
      "minimumSystemVersion": "10.13.0",
      "identity": null
    },
    "dmg": {
      "sign": false
    }
  }
}
